{
  "name": "@server-status/api-plugin-pm2",
  "version": "0.0.1",
  "description": "information about running apps via pm2",
  "main": "./dist/index.js",
  "scripts": {
    "start": "nodemon --exec babel-node dev/server.js",
    "build": "babel src --out-dir dist",
    "prebuild": "yarn clear",
    "postbuild": "find src/ -type f | grep -i .gql$ | xargs -i cp {} dist/",
    "prepublish": "yarn build",
    "serve": "PROD=1 node dev/server.js",
    "clear": "rm -fr dist",
    "lint": "eslint ."
  },
  "keywords": [
    "server",
    "status",
    "monitor",
    "stats",
    "pm2"
  ],
  "author": "Adam Zielonka <root@adamzielonka.pro>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/server-status/api-plugin-pm2"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/node": "^7.6.3",
    "@babel/preset-env": "^7.6.3",
    "eslint": "^6.6.0",
    "nodemon": "^1.19.4"
  },
  "peerDependencies": {
    "@server-status/api": "^0.1.4"
  },
  "dependencies": {
    "regenerator-runtime": "^0.13.3"
  }
}
