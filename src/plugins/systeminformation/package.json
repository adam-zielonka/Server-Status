{
  "name": "@server-status/api-plugin-systeminformation",
  "version": "0.0.9",
  "description": "plugin for server-status for retrieving server status information",
  "main": "./dist/index.js",
  "scripts": {
    "start": "nodemon --exec babel-node dev/server.js",
    "build": "yarn clear && babel src --out-dir dist && find src/ -type f | grep -i .gql$ | xargs -i cp {} dist/",
    "prepublish": "yarn build",
    "serve": "PROD=1 node dev/server.js",
    "clear": "rm -fr dist",
    "lint": "eslint ."
  },
  "keywords": [
    "server",
    "status",
    "monitor",
    "stats",
    "systeminformation"
  ],
  "author": "Adam Zielonka <root@adamzielonka.pro>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/server-status/api-plugin-systeminformation"
  },
  "dependencies": {
    "systeminformation": "^4.14.17"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/node": "^7.6.3",
    "@babel/preset-env": "^7.6.3",
    "@server-status/api": "^0.1.6",
    "eslint": "^6.6.0",
    "nodemon": "^1.19.4"
  },
  "peerDependencies": {
    "@server-status/api": "^0.1.4"
  }
}
