{
  "name": "@server-status/api",
  "version": "0.1.9",
  "description": "api for retrieving server status information",
  "main": "./dist/index.js",
  "scripts": {
    "start": "nodemon --exec babel-node src/start.js",
    "build": "babel src --out-dir dist",
    "prebuild": "yarn clear",
    "postbuild": "find src/ -type f | grep -i .gql$ | cut -c 5- | xargs -i cp src/{} dist/{}",
    "prepublish": "yarn build",
    "serve": "node dist/start.js",
    "clear": "rm -fr dist",
    "test": "jest",
    "lint": "eslint ."
  },
  "keywords": [
    "server",
    "status",
    "monitor",
    "stats"
  ],
  "author": "Adam Zielonka <root@adamzielonka.pro>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/server-status/server-status-api"
  },
  "dependencies": {
    "apollo-server": "^2.9.7",
    "graphql": "^14.5.8"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/node": "^7.6.3",
    "@babel/preset-env": "^7.6.3",
    "eslint": "^6.6.0",
    "jest": "^24.9.0",
    "nodemon": "^1.19.4"
  }
}
